<Window x:Class="ApplicationInterface.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApplicationInterface"
        mc:Ignorable="d"
        Title="SmartTicket" Height="270" Width="300" ResizeMode="NoResize">
    <Grid>
        <Button Name="AdminButton" Content="Enter as Admin" Margin="0,10,0,0" Height="100" Width="250" VerticalAlignment="Top" Click="AdminButton_Click"></Button>
        <Button Name="UserButton" Content="Enter as User" Margin="0,0,0,10" Height="100" Width="250" VerticalAlignment="Bottom"></Button>
    </Grid>
</Window>
